{
  "name": "Selenium Converter",
  "description": "Selenium Converter",
  "manifest_version": 2,
  "version": "0.1",
  "browser_action": {
    "default_title" : "Selenium Converter",
    "default_icon": "/img/icon128.png",
    "default_popup": "/html/popup.html"
  },
  "content_scripts" : [{
    "matches": ["<all_urls>"],
    "js" : [
      "/js/locator.js"
    ]
  }],
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "icons": {
    "48": "/img/icon48.png"
  }
}